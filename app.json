{
  "expo": {
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "icon": "./assets/icon.png",
    "ios": {
      "supportsTablet": true
    },

    "name": "FranFran-Final",
    "scheme": "franfran-cos",
    "orientation": "portrait",
    "plugins": [
      [
        "expo-font",
        {
          "fonts": [
            {
              "asset": "./assets/fonts/*"
            }
          ]
        }
      ],
      "expo-router"
    ],
    "slug": "FranFran-Final",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "userInterfaceStyle": "automatic",
    "version": "1.0.0",
    "web": {
      "bundler": "metro",
      "favicon": "./assets/favicon.png"
    }
  }
}
